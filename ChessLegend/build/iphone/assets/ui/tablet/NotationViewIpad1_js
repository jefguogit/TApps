function NotationView(e,t){Ti.API.log("notation view :"+e+"  "+t);{var s=myChess.chessObj.game,a=Ti.UI.createView({top:0}),o=Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:30,top:0,height:20,width:180,text:s.Event}),r=Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:30,top:20,height:20,width:120,text:s.Site}),n=Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:150,top:40,height:20,width:80,text:s.Date.substr(0,4)}),h=(Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:60,top:60,height:20,width:120,text:s.White}),Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:150,top:60,height:20,width:40,text:s.WhiteElo}),Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normarl",fontFamily:"Arial"},left:60,top:80,height:20,width:120,text:s.Black}),Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:150,top:80,height:20,width:40,text:s.BlackElo}),Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:30,top:60,height:20,width:40,text:s.ECO}));Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:110,top:60,height:20,width:40,text:s.Result})}a.add(o),a.add(r),a.add(n),a.add(h);var c=[];for(i=0;i<myChess.chessObj.moves.length;i++){var d=getRowData(myChess.chessObj.moves[i]);d.move=i,d.no.text=i,d.w.index=2*i+1,d.b.index=2*i+2,d.w.backgroundColor="white",d.b.backgroundColor="white",d.w.addEventListener("click",function(e){var t=e.source.index;Ti.API.log("notation white click"+t),myChess.f.moveTo(t)}),d.b.addEventListener("click",function(e){var t=e.source.index;Ti.API.log("notation black click"+t),myChess.f.moveTo(t)}),c.push(d)}myChess.chessObj.notations=c;var l=150,m=Ti.UI.createTableView({data:c,width:l,top:80,height:t-10,left:3,backgroundColor:"transparent",allowsSelection:!1});return a.add(m),a.table=m,m.addEventListener("show",function(e){m.scrollToIndex(e.index)}),myChess.chessObj.notation=m,a}function getRowData(e){var t=Ti.UI.createTableViewRow({backgroundColor:"white"});t.backgroundColor="transparent";var i=13;"android"==Titanium.Platform.name&&(i=12);var s=Ti.UI.createLabel({color:"black",font:{fontSize:10,fontWeight:"normal",fontFamily:"Arial"},left:60,top:0,height:20,width:25,text:1,borderWidth:1,borderColor:"#666666",backgroundColor:"ccddee",textAlign:"center"});t.add(s),t.no=s;var a=Ti.UI.createButton({color:"purple",font:{fontSize:i,fontWeight:"bold",fontFamily:"Arial"},left:0,top:0,height:20,width:60,clickName:"whitem",title:e[0],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});t.add(a);var o=Ti.UI.createButton({color:"black",font:{fontSize:i,fontWeight:"bold",fontFamily:"Arial"},left:85,right:10,top:0,height:20,width:60,clickName:"blackm",title:e[1],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});return t.add(o),t.w=a,t.b=o,t}module.exports=NotationView;