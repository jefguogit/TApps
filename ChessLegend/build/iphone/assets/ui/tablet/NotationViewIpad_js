function NotationView(e,t){Ti.API.log("notation view :"+e+"  "+t);var s=(myChess.chessObj.game,Ti.UI.createView({top:0})),a=[];for(i=0;i<myChess.chessObj.moves.length;i++){var r=getRowData(myChess.chessObj.moves[i]);r.move=i,r.no.text=i,r.w.index=2*i+1,r.b.index=2*i+2,r.w.backgroundColor="white",r.b.backgroundColor="white",r.w.addEventListener("click",function(e){var t=e.source.index;Ti.API.log("notation white click"+t),myChess.f.moveTo(t)}),r.b.addEventListener("click",function(e){var t=e.source.index;Ti.API.log("notation black click"+t),myChess.f.moveTo(t)}),a.push(r)}for(i=myChess.chessObj.moves.length;29>i;i++){var r=getBlankRowData();r.move=i,r.no.text=i+1,r.w.index=2*i+1,r.b.index=2*i+2,r.w.backgroundColor="white",r.b.backgroundColor="white",a.push(r)}myChess.chessObj.notations=a;var o=150,n=Ti.UI.createTableView({data:a,width:o,top:25,height:t,left:3,backgroundColor:"red",allowsSelection:!1});return s.add(n),s.table=n,n.addEventListener("show",function(e){n.scrollToIndex(e.index)}),myChess.chessObj.notation=n,s}function getRowData(e){var t=Ti.UI.createTableViewRow({backgroundColor:"white"});t.backgroundColor="transparent";var i=13;"android"==Titanium.Platform.name&&(i=12);var s=Ti.UI.createLabel({color:"black",font:{fontSize:10,fontWeight:"normal",fontFamily:"Arial"},left:60,top:0,height:20,width:25,text:1,borderWidth:1,borderColor:"#666666",backgroundColor:"ccddee",textAlign:"center"});t.add(s),t.no=s;var a=Ti.UI.createButton({color:"purple",font:{fontSize:i,fontWeight:"bold",fontFamily:"Arial"},left:0,top:0,height:20,width:60,clickName:"whitem",title:e[0],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});t.add(a);var r=Ti.UI.createButton({color:"black",font:{fontSize:i,fontWeight:"bold",fontFamily:"Arial"},left:85,right:10,top:0,height:20,width:60,clickName:"blackm",title:e[1],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});return t.add(r),t.w=a,t.b=r,t}function getBlankRowData(){var e=Ti.UI.createTableViewRow({backgroundColor:"white"});e.backgroundColor="transparent";var t=13;"android"==Titanium.Platform.name&&(t=12);var i=Ti.UI.createLabel({color:"black",font:{fontSize:10,fontWeight:"normal",fontFamily:"Arial"},left:60,top:0,height:20,width:25,text:1,borderWidth:1,borderColor:"#666666",backgroundColor:"ccddee",textAlign:"center"});e.add(i),e.no=i;var s=Ti.UI.createButton({color:"purple",font:{fontSize:t,fontWeight:"bold",fontFamily:"Arial"},left:0,top:0,height:20,width:60,clickName:"whitem",borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});e.add(s);var a=Ti.UI.createButton({color:"black",font:{fontSize:t,fontWeight:"bold",fontFamily:"Arial"},left:85,right:10,top:0,height:20,width:60,clickName:"blackm",borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});return e.add(a),e.w=s,e.b=a,e}module.exports=NotationView;