function IntroView(e,t){Ti.API.log("intro view :"+e+"  "+t);var s=(7*(myChess.width-50)/10*1/3,Math.floor(7*myChess.width/10*2/3-50),Ti.UI.createView({width:e,height:t,backgroundImage:Ti.Filesystem.resourcesDirectory+"images/introback.png"})),i=Ti.UI.createWebView({backgroundColor:"transparent",top:108,bottom:10,left:20,right:20});s.add(i);var a=Ti.UI.createButton({top:15,left:10,width:100,height:55,backgroundImage:null});return a.addEventListener("click",function(){s.fireEvent("backToList")}),s.add(a),s.addEventListener("refresh",function(e){var t=myChess.Players[myChess.PlayerIndex];i.html=0==e.idx?"<div>"+t[5]+"</div>":"<div>"+t[7]+"</div>"}),s.addEventListener("clean",function(){i.html="<div></div>"}),s}module.exports=IntroView;