function ApplicationWindow(){var e=3*myChess.width/10,t=myChess.height,s=5,i=myChess.width-e,a=myChess.height,r=30,o=myChess.width,n=myChess.height;Ti.API.log("size:"+myChess.width+" "+myChess.height),Ti.API.log("detail:"+i+"  "+r),Ti.API.log("master:"+e+"  "+s);{var h=require("ui/tablet/MenuViewIpad"),c=require("ui/tablet/MasterViewIpad"),d=require("ui/tablet/DetailViewIpad"),b=Ti.UI.createView({title:"Chess World Champions",bottom:0,left:-3e3,height:myChess.height,width:myChess.width,backgroundColor:"black"}),l=Ti.Media.createVideoPlayer({url:Ti.Filesystem.resourcesDirectory+"mydata/video/ccgc.m4v",width:600,mediaControlStyle:Ti.Media.VIDEO_CONTROL_FULLSCREEN,autoplay:!1});l.addEventListener("complete",function(){})}b.add(l),b.actmovie=l;var m=Ti.UI.createButton({right:10,top:38,width:120,height:90,backgroundImage:Ti.Filesystem.resourcesDirectory+"images/skipButton.png"});m.addEventListener("click",function(){Ti.API.info("skip..."),null!=myChess.appWin.vcontent&&(myChess.appWin.vv.remove(myChess.appWin.vcontent),myChess.appWin.vcontent=null),null!=myChess.appWin.vmsg&&(myChess.appWin.vv.remove(myChess.appWin.vmsg),myChess.appWin.vmsg=null),null!=myChess.appWin.webv&&(myChess.appWin.vv.remove(myChess.appWin.webv),myChess.appWin.webv=null),null!=myChess.appWin.vv.actmovie&&(myChess.appWin.vv.actmovie.stop(),myChess.appWin.vv.remove(C.vv.actmovie),myChess.appWin.vv.actmovie=null),C.fireEvent("hideHead"),C.fireEvent("showMenu")}),b.add(m),b.button=m;var f=Ti.UI.createView({title:"Chess World Champions",bottom:0,left:0,height:myChess.height,width:myChess.width}),u=Ti.UI.createView({bottom:0,left:0,height:myChess.height,width:myChess.width,navBarHidden:!0,backgroundImage:Ti.Filesystem.resourcesDirectory+"images/gameback.png"}),p=new h(o,n),g=new c(e,a),y=new d(i,a);f.add(p);var v=Ti.UI.createView({left:0,top:0,width:i,height:a});v.add(y);var w=Ti.UI.createView({right:0,top:5,width:e,height:t});w.add(g),u.add(w),u.add(v),p.addEventListener("itemSelected",function(e){g.fireEvent("display",e),y.fireEvent("showIntro",e),C.fireEvent("hideMenu")}),g.addEventListener("opengame",function(e){y.fireEvent("opengame",e)}),g.addEventListener("showIntro",function(){y.fireEvent("showIntro")}),g.addEventListener("hideIntro",function(){y.fireEvent("hideIntro")}),y.addEventListener("backToList",function(){p.cover&&(p.cover.left=-1500,p.cover.top=-900,p.left=0,p.top=0),this.fireEvent("hideIntro"),C.fireEvent("showMenu")});var C=Ti.UI.createWindow({width:myChess.width,height:myChess.height});return C.add(u),C.content=u,C.add(f),C.menu=f,C.add(b),C.hv=l,C.vv=b,C.addEventListener("hideMenu",function(){f.left=-3e3}),C.addEventListener("showMenu",function(){f.left=0}),C.addEventListener("hideHead",function(){b.left=-3e3}),C.addEventListener("showHead",function(){b.left=0}),myChess.appWin=C,C.master=g,C}module.exports=ApplicationWindow;