function MenuView(e,s){var a=Ti.UI.createView({width:e,height:s,backgroundImage:null}),r=Ti.UI.createView({width:e,height:s,backgroundImage:Ti.Filesystem.resourcesDirectory+"mydata/icon/chessback1.png",item:null}),n=Math.floor(1*myChess.width/6),h=Math.floor(1*myChess.height/4),c=Math.floor(1*myChess.width/60),o=Math.floor(1*myChess.height/60),b=Math.floor(1*myChess.width/100),f=Math.floor(1*myChess.height/100),d=myChess.Players.length,u=new Array(d);for(i=0;d>i;i++){var p=myChess.Players[i],w=Math.floor(i/5),m=i%5,v=Ti.UI.createButton({width:n,height:h,left:c+m*n+m*b,top:o+w*h+w*f,index:i,borderWidth:2,borderColor:"red",backgroundImage:Ti.Filesystem.resourcesDirectory+"mydata/icon/"+p[6]+".png"});u[i]=v}for(a.mics=u,i=0;d>i;i++){var g=a.mics[i];g.addEventListener("click",function(i){myChess.PlayerIndex=i.source.index,a.fireEvent("itemSelected",i);var n=a.mics[myChess.PlayerIndex];l=n.left,t=n.top,r.add(n),a.add(r),n.animate({left:3*e/8,top:3*s/8,duration:1e3},function(){a.add(n),n.left=l,n.top=t,r.remove(n),a.remove(r)})}),a.add(g)}return a}function goPlayer(e,s){e.fireEvent("itemSelected",{name:player[0],game:s})}function getMenuData(e){var s=Ti.UI.createTableViewRow();s.selectedBackgroundColor="#fff",s.className="datarow",s.clickName="row",s.backgroundImage="images/playerback.png",s.height=120,s.name=e[0];var t=Ti.Filesystem.resourcesDirectory+e[6],i=Ti.UI.createImageView({image:t,width:"auto",height:100,top:6,left:20});s.add(i);var a=Ti.UI.createLabel({color:"#ffffff",font:{fontSize:18,fontWeight:"bold",fontFamily:"Arial"},left:200,top:2,height:30,width:"auto",text:e[0]+",  "+e[2]+" | "+e[1]});s.add(a);var r=16;"android"==Titanium.Platform.name&&(r=14);var n=Ti.UI.createLabel({color:"blue",font:{fontSize:r,fontWeight:"normal",fontFamily:"Arial"},left:180,top:31,height:"auto",width:"auto",clickName:"subtitle",text:e[4]});return s.add(n),s}function getMenuItem(e){var s=Ti.UI.createView({width:1*myChess.width/4,height:1*myChess.height/4}),t=Ti.UI.createView({top:0,left:0,width:1*myChess.width/10,height:1*myChess.height/4}),i=Ti.Filesystem.resourcesDirectory+"mydata/pic/"+e[6]+".jpg",a=Ti.UI.createImageView({image:i,width:2*myChess.width/25,height:1*myChess.height/6,top:1,left:1});t.add(a);var r=Ti.UI.createLabel({color:"#000000",font:{fontSize:15,fontWeight:"bold",fontFamily:"Arial"},left:1,top:1*myChess.height/6+3,height:1*myChess.height/12,width:2*myChess.width/25,text:e[0]+",  "+e[2]+" | "+e[1]});t.add(r);var n=Ti.UI.createView({width:3*myChess.width/20,height:1*myChess.width/4,left:1*myChess.width/10,top:0}),h=12;"android"==Titanium.Platform.name&&(h=11);var c=Ti.UI.createLabel({color:"blue",font:{fontSize:h,fontWeight:"normal",fontFamily:"Arial"},left:1,top:1,height:1*myChess.height/4,width:3*myChess.width/20,text:e[4]});n.add(c),s.add(t),s.add(n);var o=Ti.UI.createTableViewRow();return o.add(s),s}module.exports=MenuView;