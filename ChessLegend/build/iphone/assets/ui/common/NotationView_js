function NotationView(){var e=myChess.chessObj.game,s=Ti.UI.createView({bottom:20}),t=(Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:20,top:2,height:20,width:60,text:"WHITE: "}),Ti.UI.createLabel({font:{fontSize:12,fontWeight:"bold",fontFamily:"Arial"},left:45,top:20,height:20,width:180,text:e.White}));s.add(t),s.white=t;var a=(Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:30,top:30,height:20,width:60,text:"BLACK: "}),Ti.UI.createLabel({font:{fontSize:12,fontWeight:"bold",fontFamily:"Arial"},left:220,top:20,height:20,width:180,text:e.Black}));s.add(a),s.black=a;var r=Ti.UI.createLabel({color:"#001122",font:{fontSize:12,fontWeight:"bold",fontFamily:"Arial"},left:45,top:3,height:20,width:180,text:e.Site});s.add(r);var n=Ti.UI.createLabel({color:"#001122",font:{fontSize:12,fontWeight:"bold",fontFamily:"Arial"},left:200,top:3,height:20,width:180,text:e.Date});s.add(n);var h=Ti.UI.createLabel({color:"black",font:{fontSize:12,fontWeight:"bold",fontFamily:"Arial"},left:300,top:3,height:20,width:180,clickName:"result",text:e.Result});s.add(h),s.result=h;var o=[];for(i=0;i<myChess.chessObj.moves.length;i++){var c=getRowData(myChess.chessObj.moves[i]);c.move=i,c.no.text=i,c.w.index=2*i+1,c.b.index=2*i+2,c.w.addEventListener("click",function(e){var s=e.source.index;Ti.API.log("notation white click"+s),myChess.f.moveTo(s)}),c.b.addEventListener("click",function(e){var s=e.source.index;Ti.API.log("notation black click"+s),myChess.f.moveTo(s)}),o.push(c)}myChess.chessObj.notations=o;var b=95,f=Ti.UI.createTableView({data:o,width:b,top:50,bottom:10,right:0,backgroundColor:"transparent",allowsSelection:!1});return s.add(f),s.table=f,f.addEventListener("show",function(e){f.scrollToIndex(e.index)}),myChess.chessObj.notation=f,s}function getRowData(e){var s=Ti.UI.createTableViewRow({backgroundColor:"transparent"});s.backgroundColor="transparent";var t=9;"android"==Titanium.Platform.name&&(t=8);var i=Ti.UI.createLabel({color:"black",font:{fontSize:6,fontWeight:"normal",fontFamily:"Arial"},left:37,top:0,height:20,width:15,text:1,borderWidth:1,borderColor:"#666666",textAlign:"center"});s.add(i),s.no=i;var a=Ti.UI.createButton({color:"purple",font:{fontSize:t,fontWeight:"bold",fontFamily:"Arial"},left:0,top:0,height:20,width:38,clickName:"whitem",title:e[0],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});s.add(a);var r=Ti.UI.createButton({color:"black",font:{fontSize:t,fontWeight:"bold",fontFamily:"Arial"},left:52,right:10,top:0,height:20,width:40,clickName:"blackm",title:e[1],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});return s.add(r),s.w=a,s.b=r,s}module.exports=NotationView;