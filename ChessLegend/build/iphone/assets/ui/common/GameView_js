function GameView(e,s){Ti.API.log("game view :"+e+"  "+s);var i=150,t=e-i,c=Ti.UI.createView({top:t+20,left:10+i,bottom:2}),n=Ti.UI.createView({top:30,left:10,width:i,height:s-20}),r=require("ui/tablet/NotationViewIpad"),h=new r(i,s-22);h.addEventListener("moveToLine",function(e){e.move;moveForward()}),n.add(h);var a=Ti.UI.createView({top:70,left:10+i,width:t,height:t});a.add(myChess.chessObj.board);var b=Ti.UI.createView({width:e,height:s}),f=require("ui/common/ControlView"),o=new f(myChess.chessObj.board.width-10,80);return c.add(o),b.add(a),b.add(n),b.notation=h,b.add(c),b.control=o,b.addEventListener("openAgame",function(e){myChess.chessObj.gameIndex=e.game,myChess.chessObj.game=myChess.Games[myChess.PlayerIndex].games[myChess.chessObj.gameIndex],myChess.f.getPiecesStatus(),myChess.f.resetPieces(),b.NotationView||(n.remove(b.notation),games=myChess.Games,game=games[e.game],b.notation=new r(i,t),n.add(b.notation))}),b.fireEvent("openAgame",{game:myChess.chessObj.gameIndex}),b}module.exports=GameView;