function IntroView(e,s){Ti.API.log("intro view :"+e+"  "+s);var i=7*(myChess.width-50)/10*1/3,t=Math.floor(7*myChess.width/10*2/3-50),c=Ti.UI.createView({top:30,left:10,width:i,height:s-20}),n=myChess.Players[myChess.PlayerIndex],r=Ti.UI.createLabel({text:n[0]});c.add(r);var h=Ti.UI.createView({top:70,left:10+i,width:t,height:t}),a=Ti.UI.createLabel({width:t-50,top:50,left:50,text:n[5]});h.add(a);var b=Ti.UI.createView({width:e,height:s,backgroundImage:Ti.Filesystem.resourcesDirectory+"images/clb.png"});return b.add(h),b.add(c),b.addEventListener("refresh",function(){var e=myChess.Players[myChess.PlayerIndex];r.text=e[0],a.text=e[5]}),b}module.exports=IntroView;