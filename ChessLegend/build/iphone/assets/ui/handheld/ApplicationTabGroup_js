function ApplicationTabGroup(){var e=Ti.UI.createTabGroup(),s=require("ui/handheld/MenuViewIphone"),t=require("ui/handheld/MasterViewIphone"),i=require("ui/handheld/DetailViewIphone"),a=require("ui/handheld/IntroViewIphone"),r=new s,n=new t,o=new i,h=new a;r.top=20,n.top=20,o.top=20,h.top=20;var c=Ti.UI.createWindow({top:16,navBarHidden:!0,tabBarHidden:!0}),b=Ti.UI.createButton({left:0,top:0,width:80,height:45,backgroundImage:null});b.addEventListener("click",function(){Ti.API.info("done..."),myChess.tabGroup.vcontent&&myChess.tabGroup.vv.remove(myChess.tabGroup.vcontent),myChess.tabGroup.vmsg&&myChess.tabGroup.vv.remove(myChess.tabGroup.vmsg),e.fireEvent("showPlayer")}),c.add(b),e.vv=c,e.button=b;var f=Ti.UI.createWindow({backgroundImage:Ti.Filesystem.resourcesDirectory+"images/chessback.png",navBarHidden:!0,tabBarHidden:!0,fullscreen:!1});f.add(r);var d=Ti.UI.createWindow({backgroundImage:Ti.Filesystem.resourcesDirectory+"images/chessback.png",navBarHidden:!0,tabBarHidden:!0});d.add(h),e.iv=h;var p=Ti.UI.createWindow({backgroundImage:Ti.Filesystem.resourcesDirectory+"images/chessback.png",navBarHidden:!0,tabBarHidden:!0});p.add(n),e.mv=n;var m=Ti.UI.createWindow({backgroundImage:Ti.Filesystem.resourcesDirectory+"images/chessback.png",navBarHidden:!0,tabBarHidden:!0});m.add(o),e.gv=o;var u=Ti.UI.createTab({title:L("player List"),icon:"/images/KS_nav_ui.png",window:f});f.containingTab=u;var w=Ti.UI.createTab({title:L("introduction"),icon:"/images/KS_nav_views.png",window:d});d.containingTab=w;var v=Ti.UI.createTab({title:L("game List"),icon:"/images/KS_nav_ui.png",window:p});p.containingTab=v;var l=Ti.UI.createTab({title:L("game detail"),icon:"/images/KS_nav_views.png",window:m});m.containingTab=l;var y=Ti.UI.createTab({title:L("Head Leader"),icon:"/images/KS_nav_views.png",window:c});return c.containingTab=y,e.addTab(u),e.addTab(w),e.addTab(v),e.addTab(l),e.addTab(y),e.addEventListener("showHead",function(){Ti.API.info("showHead"),this.setActiveTab(4)}),e.addEventListener("showPlayer",function(){Ti.API.info("showPlayer"),this.setActiveTab(0)}),e.addEventListener("showIntro",function(e){Ti.API.info("showIntro"),this.iv.fireEvent("refresh",e),this.setActiveTab(1)}),e.addEventListener("showMaster",function(){Ti.API.info("showMaster"),this.setActiveTab(2),this.mv.fireEvent("display")}),e.addEventListener("showGame",function(){this.setActiveTab(3),this.gv.fireEvent("opengame")}),e}module.exports=ApplicationTabGroup;