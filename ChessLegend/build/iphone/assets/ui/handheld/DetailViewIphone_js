function DetailView(e,s){var t=require("ui/handheld/GameViewIphone"),i=Ti.UI.createView({width:e,height:s}),a=new t(e,s-10);i.add(a),i.gv=a;var r=Ti.UI.createButton({width:65,height:45,top:2,left:5,backgroundImage:null});return r.addEventListener("click",function(){myChess.chessObj.playModeInProgress=!1,a.control.backgroundImage=Ti.Filesystem.resourcesDirectory+"images/chessControlPlay.png",myChess.tabGroup.fireEvent("showMaster")}),i.add(r),i.addEventListener("opengame",function(){Ti.API.log("fire detail "+myChess.chessObj.gameIndex),a.fireEvent("openAgame")}),i}module.exports=DetailView;