function IntroView(e,s){var t=Ti.UI.createView({width:e,height:s,backgroundColor:"green",backgroundImage:Ti.Filesystem.resourcesDirectory+"images/introbackh.png"}),i=Ti.UI.createView({top:0,left:0,width:e,height:50}),a=Ti.UI.createView({left:0,top:50,width:e,height:s-60});t.add(i),t.add(a);var r=Ti.UI.createButton({top:0,left:0,width:80,height:50,backgroundImage:null});r.addEventListener("click",function(){myChess.tabGroup.fireEvent("showPlayer")}),t.add(r);var o=Ti.UI.createWebView({top:60,left:20,right:20,backgroundColor:"transparent"});return o.html=" ",t.add(o),t.addEventListener("refresh",function(e){var s=myChess.Players[myChess.PlayerIndex];o.html=0==e.idx?s[5]:s[7]}),t}module.exports=IntroView;