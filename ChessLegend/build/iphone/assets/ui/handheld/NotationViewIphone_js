function NotationView(){var e=myChess.chessObj.game,t=Ti.UI.createView({top:0}),s="";e.Event&&-1==e.Event.indexOf("?")?s=e.Event:e.Site&&-1==e.Site.indexOf("?")&&(s=e.Site);var a=Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:115,top:100,height:20,width:50,text:"EVENT:"}),r=Ti.UI.createLabel({font:{fontSize:15,fontWeight:"normal",fontFamily:"Arial"},color:"blue",left:158,top:100,height:20,width:150,text:s}),o=(Ti.UI.createLabel({font:{fontSize:15,fontWeight:"normal",fontFamily:"Arial"},left:115,top:120,height:20,width:180,text:e.Site}),e.Year);e.Date&&e.Date.length>0&&(o=e.Year+"."+e.Date);var n=Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:115,top:125,height:20,width:45,text:"DATE:"}),h=Ti.UI.createLabel({font:{fontSize:15,fontWeight:"normal",fontFamily:"Arial"},color:"blue",left:150,top:125,height:20,width:100,text:o}),c=Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:115,top:150,height:20,width:38,text:"ECO:"}),d=Ti.UI.createLabel({font:{fontSize:15,fontWeight:"normal",fontFamily:"Arial"},color:"blue",left:145,top:150,height:20,width:100,text:e.ECO}),b=Ti.UI.createLabel({font:{fontSize:12,fontWeight:"normal",fontFamily:"Arial"},left:245,top:130,height:20,width:55,text:"RESULT:"}),l=Ti.UI.createLabel({font:{fontSize:15,fontWeight:"normal",fontFamily:"Arial"},color:"blue",left:245,top:150,height:20,width:60,text:e.Result});t.add(a),t.add(r),t.add(n),t.add(h),t.add(c),t.add(d),t.add(b),t.add(l);var f=[];for(i=0;i<myChess.chessObj.moves.length;i++){var m=getRowData(myChess.chessObj.moves[i]);m.move=i,m.no.text=i+1,m.w.index=2*i+1,m.b.index=2*i+2,m.w.backgroundColor="white",m.b.backgroundColor="white",m.w.addEventListener("click",function(e){var t=e.source.index;Ti.API.log("notation white click"+t),myChess.f.moveTo(t)}),m.b.addEventListener("click",function(e){var t=e.source.index;Ti.API.log("notation black click"+t),myChess.f.moveTo(t)}),f.push(m)}for(i=0;8>i;i++){var m=getBlankRowData();f.push(m)}myChess.chessObj.notations=f;var u=100,p=Ti.UI.createTableView({data:f,width:u,top:5,height:165,left:8,backgroundColor:"transparent",separatorColor:"transparent",allowsSelection:!1});return t.add(p),t.table=p,p.addEventListener("show",function(e){p.scrollToIndex(e.index)}),myChess.chessObj.notation=p,t}function getRowData(e){var t=Ti.UI.createTableViewRow({backgroundColor:"white"});t.backgroundColor="transparent";var s=12,i=Ti.UI.createLabel({color:"black",font:{fontSize:7,fontWeight:"normal",fontFamily:"Arial"},left:43,top:0,height:20,width:12,text:1,borderWidth:1,borderColor:"#666666",backgroundColor:"ccddee",textAlign:"center"});t.add(i),t.no=i;var a=Ti.UI.createButton({color:"purple",font:{fontSize:s,fontWeight:"bold",fontFamily:"Arial"},left:0,top:0,height:20,width:43,clickName:"whitem",title:e[0],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});t.add(a);var r=Ti.UI.createButton({color:"black",font:{fontSize:s,fontWeight:"bold",fontFamily:"Arial"},left:55,right:10,top:0,height:20,width:43,clickName:"blackm",title:e[1],borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});return t.add(r),t.w=a,t.b=r,t}function getBlankRowData(){var e=Ti.UI.createTableViewRow({backgroundColor:"white"});e.backgroundColor="transparent";var t=9;"android"==Titanium.Platform.name&&(t=8);var s=Ti.UI.createLabel({color:"black",font:{fontSize:7,fontWeight:"normal",fontFamily:"Arial"},left:43,top:0,height:20,width:20,text:" ",borderWidth:1,borderColor:"#666666",backgroundColor:"ccddee",textAlign:"center"});e.add(s),e.no=s;var i=Ti.UI.createButton({color:"purple",font:{fontSize:t,fontWeight:"bold",fontFamily:"Arial"},left:0,top:0,height:20,width:43,clickName:"whitem",title:" ",borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});e.add(i);var a=Ti.UI.createButton({color:"black",font:{fontSize:t,fontWeight:"bold",fontFamily:"Arial"},left:55,right:10,top:0,height:20,width:43,clickName:"blackm",title:" ",borderWidth:1,borderColor:"#666666",textAlign:"center",backgroundImage:null});return e.add(a),e.w=i,e.b=a,e}module.exports=NotationView;